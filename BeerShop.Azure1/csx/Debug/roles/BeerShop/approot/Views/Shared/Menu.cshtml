@model IEnumerable<BeerShop.Models.Category>
@{
    ViewBag.Title = "Menu";
}


<ul class="nav nav-list">

@foreach(var c in Model)
{
    
    <li class="nav-header">
    <h4><strong>@Html.DisplayFor(Modelitem => c.name)</strong></h4>
    </li>
    
    {Html.RenderPartial("_ItemsCategory",c.categories);}   
    <br />
}
</ul>




